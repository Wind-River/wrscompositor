{
    "Layout": {
        "Hmi": {
            "StatusBar": {
                "scale": "100x10",
                "position": "topCenter",
                "opacity": 1.0,
                "animation": true,
                "layer": "base"
            },
            "DockBar": {
                "scale": "100x10",
                "position": "bottomCenter",
                "opacity": 1.0,
                "animation": true,
                "layer": "base"
            },
            "SidePanelMedia": {
                "scale":"30x40",
                "position": "middleRightTop",
                "opacity": 1.0,
                "animation": true,
                "layer" : "base"
            },
            "SidePanelHVAC": {
                "scale": "30x40",
                "position": "middleRightBottom",
                "opacity": 1.0,
                "animation": true,
                "layer": "base"
            },
            "WallPaper": {
                "scale": "70x80",
                "position": "middleLeft",
                "opacity": 1.0,
                "animation": true,
                "layer": "base"
            }
        },
        "Workspace": {
            "Default": {
                "scale": "70x80",
                "position": "middleLeft",
                "opacity": 1.0,
                "animation": true,
                "layer": "application"
            },
            "Launcher": {
                "scale": "70x80",
                "position": "middleLeft",
                "opacity": 0.8,
                "animation": true,
                "layer": "home"
            },
            "Widget": {
                "scale":"30x40",
                "position": "middleRightTop",
                "opacity": 1.0,
                "animation": true,
                "layer": "application"
            },
            "Popup": {
                "scale": "40x40",
                "position": "undefined",
                "x": 500,
                "y": 400,
                "opacity": 1.0,
                "animation": true,
                "layer": "notification"
            },
            "FullScreen": {
                "scale": "100x100",
                "position": "middleCenter",
                "opacity": 1.0,
                "animation": true,
                "layer": "notification"
            }
        }
    },
    "Role": {
        "Default": {
            "parent": "Default",
            "id": 0
        },
        "Navigation": {
            "parent": "Popup",
            "id": 1
        },
        "Dialog": {
            "parent": "Popup",
            "id": 2
        },
        "Phone": {
            "parent": "Widget",
            "id": 3
        },
        "Projection": {
            "parent": "FullScreen",
            "id": 4
        },
        "Camera": {
            "parent": "FullScreen",
            "id": 5
        }
    },
    "Launcher": {
        "Web Browsing": {
            "icon" : "icons/native-web.png",
            "path" : "/usr/bin/brower",
            "unitFile": "brower.service",
            "multiple": false,
            "systemd": false
        },
        "Navigation": {
            "icon" : "icons/native-map-location.png",
            "path" : "/usr/bin/skobblernavi",
            "unitFile": "skobblernavi.service",
            "multiple": false,
            "systemd": false
        },
        "Media": {
            "icon" : "icons/native-video.png",
            "path" : "/usr/bin/mediaplayer",
            "unitFile": "mediaplayer.service",
            "multiple": false,
            "systemd": false
        },
        "Phone": {
            "icon" : "icons/native-phone.png",
            "path" : "/usr/bin/phone",
            "unitFile": "phone.service",
            "multiple": false,
            "systemd": false
        }
    }
}